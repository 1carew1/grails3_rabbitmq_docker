{
  "family": "RabbitMQGrails3",
  "volumes": [
    {
      "name": "my-vol",
      "host": {}
    }
  ],
  "containerDefinitions": [
    {
      "environment": [],
      "name": "RabbitMQGrails3",
      "image": "1carew1/grails3_rabbitmq_docker",
      "cpu": 50,
      "memory": 500,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "mountPoints": [],
      "entryPoint": [
        "java",
        "-Dgrails.env=prod",
        "-jar",
        "rabbitMQGrails3-0.1.war"
      ],
      "essential": true
    },
    {
      "name": "busybox",
      "image": "busybox",
      "cpu": 10,
      "memory": 200,
      "volumesFrom": [
        {
          "sourceContainer": "RabbitMQGrails3"
        }
      ],
      "entryPoint": [
        "sh",
        "-c"
      ],
      "command": [
        "/bin/sh -c \"while true; do /bin/date > /var/www/my-vol/date; sleep 50; done\""
      ],
      "essential": false
    }
  ]
}
